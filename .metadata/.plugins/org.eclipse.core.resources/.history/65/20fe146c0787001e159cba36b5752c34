package negocio;

import projeto.Commit;
import projeto.Issue;
import projeto.Repositorio;

public class Repositorio {
    private String nome;

    public Repositorio(String nome) {
        this.nome = nome;
    }

    public String getNome() {
        return nome;
    }
}




